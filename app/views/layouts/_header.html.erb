<header class="nav-wrapper">
  <div class="container">
    <div class="header-left">
      <%= link_to "Clone", root_path, id: "logo", class: "header-tag" %>
    </div>
    <div class="header-right">
      <nav>
        <li><%= link_to "Home", root_path, class: "header-tag" %></li>
        <li><%= link_to "Help", help_path, class: "header-tag" %></li>
        
        <% if logged_in? %>
          <li><%= link_to "Users", users_path, class: "header-tag" %></li>
          <li><%= link_to "Profile", user_path(current_user), class: "header-tag" %></li>
          <li><%= link_to "Settings", edit_user_path(current_user), class: "header-tag" %></li>
          <li><%= link_to "Logout", logout_path, method: :delete, class: "header-tag" %></li>
        <% else %>
          <li><%= link_to "Login", login_path, class: "header-tag" %></li>
        <% end %>
        
      </nav>
    </div>
    <div class="clear"></div>
  </div>
</header>
<div class="clear"></div>